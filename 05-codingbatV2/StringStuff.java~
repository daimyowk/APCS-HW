public class StringStuff.java{
    //Question1
    public String nonStart(String a, String b) {
	String partone = a;
	String parttwo = b;
	if (a.length() == 1) {
	    partone = "";
	} else {
	    partone = a.substring(1);
	}
	if (b.length() == 1) {
	    parttwo = "";
	} else {
	    parttwo = b.substring(1);
	}
	String combine = partone + parttwo;
	return combine;
    }
    //
    
