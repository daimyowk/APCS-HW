public class Driver{
    public static void main(String[] args){
	stuff test= new stuff();
	System.out.println(test.frontPiece({1,2,3}));
	System.out.println(test.frontPiece({1,2}));
	System.out.println(test.frontPiece({1}));
	System.out.println(test.sum13({1, 2, 2, 1, 13}));
	System.out.println(test.sum13({13,1,2,13,2,1,13}));
    }
}
			   
			   
